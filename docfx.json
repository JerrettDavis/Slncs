{
  "metadata": [
    {
      "src": [
        { "files": [ "Slncs/Slncs.csproj" ] },
        { "files": [ "Slncs.Sdk/Slncs.Sdk.csproj" ] },
        { "files": [ "Slncs.Tool/Slncs.Tool.csproj" ] }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "properties": {
        "TargetFramework": "net8.0"
      }
    }
  ],
  "build": {
    "content": [
      { "files": [ "docs/**.md", "docs/toc.yml" ] },
      { "files": [ "api/**.yml" ], "src": "api" }
    ],
    "resource": [
      { "files": [ "docs/images/**" ] }
    ],
    "globalMetadata": {
      "_appTitle": "Slncs",
      "_enableSearch": true,
      "_gitContribute": { "branch": "main", "repo": "https://github.com/JerrettDavis/Slncs" },
      "_gitUrlPattern": "github"
    },
    "template": [ "default" ],
    "dest": "_site",
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ]
  }
}

